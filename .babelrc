{
  "presets": [
    "@babel/env",
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties",
    [
      "prismjs",
      {
        "languages": [
          "javascript",
          "css",
          "markup",
          "jsx"
        ],
        "plugins": [
          "line-numbers"
        ],
        "theme": "default",
        "css": true
      }
    ]
  ]
}